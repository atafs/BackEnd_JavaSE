package io.scanner.ex3;


public class Movimento {
	private Data data;
	private Data dataValor;
	private String descricao;
	private double quantiaMovimentada;
	private double saldoContabilistico;
	private double saldoCorrente;
	private Categoria categoria;
	
	public Movimento(Data data, double quantiaMovimentada, double saldoCorrente) {
		this.data = data;
		this.quantiaMovimentada = quantiaMovimentada;
		this.saldoCorrente = saldoCorrente;
	}
	// …
	public Data getData() {
		return data;
	}
	public Data getDataValor() {
		return dataValor;
	}
	public String getDescricao() {
		return descricao;
	}
	public double getQuantiaMovimentada() {
		return quantiaMovimentada;
	}
	public double getSaldoContabilistico() {
		return saldoContabilistico;
	}
	public double getSaldoCorrente() {
		return saldoCorrente;
	}
	public Categoria getCategoria() {
		return categoria;
	}
	
}